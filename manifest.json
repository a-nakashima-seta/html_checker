{
    "manifest_version": 3,
    "name": "HTML Checker",
    "version": "1.0",
    "description": "Check if a specific string exists in the HTML source.",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "48": "checker48.png"
    }
}